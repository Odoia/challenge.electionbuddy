<p id="notice"><%= notice %></p>

<h1>Audit Election</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start By</th>
      <th>end at</th>
      <th>Version</th>
      <th>Status</th>
      <th>Identification</th>
      <th colspan="3"></th>
      <th>Name</th>
      <th>Version</th>
      <th>Status</th>
      <th>Identification</th>
      <th>Name</th>
      <th>Version</th>
      <th>Status</th>
      <th>Identification</th>
    </tr>
  </thead>

  <tbody>
    <% @elections.each do |election| %>
      <tr>
        <td><%= election.name %></td>
        <td><%= election.start_at.to_s %></td>
        <td><%= election.end_at.to_s %></td>
        <td><%= election.version %></td>
        <td><%= election.status %></td>
        <td><%= election.identification %></td>
      </tr>
      <% election.questions.each do |question| %>
        <tr>
          <td><%= question.name %></td>
          <td><%= question.version %></td>
          <td><%= question.status %></td>
          <td><%= question.identification %></td>
        </tr>
        <% question.answers.each do |answer| %>
          <tr>
            <td><%= answer.name %></td>
            <td><%= answer.version %></td>
            <td><%= answer.status %></td>
            <td><%= answer.identification %></td>
          </tr>
        <% end %>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Back', elections_url %>
